    "jest-coverage-badges": "^1.1.2",
    "lint-staged": "^15.2.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.3.0",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1"
